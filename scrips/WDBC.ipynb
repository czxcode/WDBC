   "source": [
    "\"\"\"\n",
    "WDBC跨语言分析项目 - Python数据处理模块\n",
    "功能：数据清洗、特征工程、预处理管道构建\n",
    "\"\"\"\n",
    "import pandas as pd\n",
    "from sklearn.impute import SimpleImputer\n",
    "from sklearn.preprocessing import StandardScaler\n",
    "from sklearn.pipeline import Pipeline\n",
    "from sklearn.compose import ColumnTransformer\n",
    "import numpy as np\n",
    "\n",
    "# 数据加载\n",
    "def load_data(file_path):\n",
    "    \"\"\"\n",
    "    加载WDBC数据集\n",
    "    :param file_path: 数据文件路径\n",
    "    :return: 清洗后的DataFrame\n",
    "    \"\"\"\n",
    "    columns = [\n",
    "        'ID', 'Diagnosis',\n",
    "        'radius_mean', 'texture_mean', 'perimeter_mean', 'area_mean', 'smoothness_mean',\n",
    "        'compactness_mean', 'concavity_mean', 'concave_points_mean', 'symmetry_mean', \n",
    "        'fractal_dimension_mean',\n",
    "        'radius_se', 'texture_se', 'perimeter_se', 'area_se', 'smoothness_se',\n",
    "        'compactness_se', 'concavity_se', 'concave_points_se', 'symmetry_se',\n",
    "        'fractal_dimension_se',\n",
    "        'radius_worst', 'texture_worst', 'perimeter_worst', 'area_worst',\n",
    "        'smoothness_worst', 'compactness_worst', 'concavity_worst',\n",
    "        'concave_points_worst', 'symmetry_worst', 'fractal_dimension_worst'\n",
    "    ]\n",
    "    \n",
    "    df = pd.read_csv(file_path, header=None, names=columns)\n",
    "    print(\"数据类型验证:\\n\", df.dtypes)\n",
    "    return df\n",
    "\n",
    "# 数据清洗\n",
    "def clean_data(df):\n",
    "    numeric_cols = df.columns.difference(['ID', 'Diagnosis'])\n",
    "    df[numeric_cols] = df[numeric_cols].apply(pd.to_numeric, errors='coerce')\n",
    "    \"\"\"处理缺失值和异常值\"\"\"\n",
    "    # 检查缺失值\n",
    "    print(\"缺失值统计:\\n\", df.isnull().sum())\n",
    "    \n",
    "    # 使用中位数填补缺失值\n",
    "    imputer = SimpleImputer(strategy='median')\n",
    "    df_imputed = pd.DataFrame(imputer.fit_transform(df.drop(['ID', 'Diagnosis'], axis=1)),\n",
    "                             columns=df.columns[2:])\n",
    "    \n",
    "    # 合并非数值列\n",
    "    cleaned_df = pd.concat([df[['ID', 'Diagnosis']], df_imputed], axis=1)\n",
    "    \n",
    "   # 仅选择数值列进行异常值检测\n",
    "    numeric_cols = df.select_dtypes(include=[np.number]).columns\n",
    "    Q1 = df[numeric_cols].quantile(0.25)\n",
    "    Q3 = df[numeric_cols].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "    \n",
    "    # 生成布尔掩码时限定数值列\n",
    "    mask = (\n",
    "        (df[numeric_cols] < (Q1 - 1.5 * IQR)) | \n",
    "        (df[numeric_cols] > (Q3 + 1.5 * IQR)\n",
    "    ))\n",
    "    cleaned_df = df[~mask.any(axis=1)]\n",
    "    \n",
    "    return cleaned_df\n",
    "\n",
    "# 特征工程\n",
    "def feature_engineering(df):\n",
    "    \"\"\"创建新特征\"\"\"\n",
    "    # 创建面积周长比特征\n",
    "    df['area_perimeter_ratio_mean'] = df['area_mean'] / df['perimeter_mean']\n",
    "    df['area_perimeter_ratio_worst'] = df['area_worst'] / df['perimeter_worst']\n",
    "    \n",
    "    # 创建恶性概率估算特征\n",
    "    df['malignancy_score'] = (df['concavity_mean'] * 0.3 + \n",
    "                             df['concave_points_mean'] * 0.4 + \n",
    "                             df['compactness_mean'] * 0.3)\n",
    "    \n",
    "    return df\n",
    "\n",
    "# 构建预处理管道\n",
    "def build_pipeline():\n",
    "    \"\"\"创建可复用的数据处理管道\"\"\"\n",
    "    numeric_features = [\n",
    "        'radius_mean', 'texture_mean', 'perimeter_mean', 'area_mean',\n",
    "        'smoothness_mean', 'compactness_mean', 'concavity_mean',\n",
    "        'concave_points_mean', 'symmetry_mean', 'fractal_dimension_mean'\n",
    "    ]\n",
    "    \n",
    "    preprocessor = ColumnTransformer(\n",
    "        transformers=[\n",
    "            ('num', StandardScaler(), numeric_features)\n",
    "        ])\n",
    "    \n",
    "    pipeline = Pipeline(steps=[\n",
    "        ('imputer', SimpleImputer(strategy='median')),\n",
    "        ('preprocessor', preprocessor)\n",
    "    ])\n",
    "    \n",
    "    return pipeline\n",
    "\n",
    "if __name__ == \"__main__\":\n",
    "    # 执行数据处理流程\n",
    "    raw_df = load_data('/Users/chen/WDBC/data/wdbc.data')\n",
    "    cleaned_df = clean_data(raw_df)\n",
    "    processed_df = feature_engineering(cleaned_df)\n",
    "    \n",
    "    # 保存处理后的数据\n",
    "    processed_df.to_csv('/Users/chen/WDBC/outputs/processed_data.csv', index=False)\n",
    "    print(\"数据处理完成，已保存为processed_data.csv\")\n",
    "\n",
    "    print(\"\\n开始人工缺失值测试...\")\n",
    "    \n",
    "    # 2. 复制原始数据用于测试\n",
    "    test_df = raw_df.copy()\n",
    "    \n",
    "    # 3. 随机注入5%的缺失值\n",
    "    np.random.seed(42)  # 固定随机种子保证可重复性\n",
    "    mask = np.random.choice(\n",
    "        [True, False], \n",
    "        size=test_df.shape, \n",
    "        p=[0.05, 0.95]\n",
    "    )\n",
    "    test_df_missing = test_df.mask(mask)\n",
    "    \n",
    "    # 4. 运行清洗流程\n",
    "    test_cleaned = clean_data(test_df_missing)\n",
    "    \n",
    "    # 5. 验证结果\n",
    "    print(\"\\n测试结果:\")\n",
    "    print(\"- 注入缺失值总数:\", test_df_missing.isnull().sum().sum())\n",
    "    print(\"- 清洗后缺失值数:\", test_cleaned.isnull().sum().sum())\n",
    "    print(\"- 原始数据量:\", len(test_df))\n",
    "    print(\"- 清洗后数据量:\", len(test_cleaned))\n",
    "    \n",
    "    # 6. 保存测试结果（可选）\n",
    "    test_cleaned.to_csv('/Users/chen/WDBC/outputs/debug_data.csv', index=False)\n",
    "    print(\"人工缺失值测试完成\")"
   ]
